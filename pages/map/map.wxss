page{
  background-color: #f8f7f3;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: flex-start;
}
map{
  width: 750rpx;
}
.query-view{
  width: 718rpx;
  height: 82rpx;
  margin: 16rpx;
  background-color: white;
  border-radius: 4rpx;
  color: rgb(160, 160, 160);
  font-size: 28rpx;
  line-height: 42rpx;
  padding-left: 32rpx;
  box-shadow: 2rpx 2rpx 2rpx #888888;
}
.query-view image{
  width: 32rpx;
  height: 32rpx;
  margin-right: 32rpx;
}