.adsimg {
  margin-top: 32rpx;
  margin-bottom: 32rpx;
  width: 180rpx;
  height: 180rpx;
  border-radius: 90rpx;
  background-color: rgb(184, 184, 184)
}
.view1 {
  padding: 32rpx;
  padding-top: 0;
  position: relative;
}
.v1-title {
  font-size: 38rpx;
  line-height: 57rpx;
  margin-top: 32rpx;
}
.v1-carnum{
  font-size: 28rpx;
  line-height: 42rpx;
  margin-top: 30rpx;
  color: #b1b1b1;
}
.v1-comm {
  font-size: 26rpx;
  line-height: 39rpx;
  color: #b1b1b1;
  margin-top: 12rpx;
}
.v1-star {
  margin-right: 12rpx;
}
.v1-star image,.v5-d1s image {
  width: 28rpx;
  height: 26rpx;
}
.v1-score {
  position: absolute;
  top: 0;
  right: 32rpx;
  width: 105rpx;
  height: 146rpx;
  background-color: #0ccef1;
  font-size: 48rpx;
  line-height: 63rpx;
  color: white;
}
.v1-score view:last-child {
  font-size: 22rpx;
  line-height: 33rpx;
}
.view2 {
  padding: 32rpx;
  font-size: 26rpx;
  line-height: 39rpx;
  color: #0ccef1;
}
.v2-comm {
  width: 216rpx;
  height: 64rpx;
  border: 3rpx solid #0ccef1;
  margin-right: 19rpx;
}
.v2-hui {
  color: #a2a2a2;
  border: 3rpx solid #c6c6c6;
}
.v2-row {
  margin-bottom: 10rpx;
}
.view2 .v2-row:last-child {
  margin-bottom: 0;
}
.v2-row .v2-comm:last-child {
  margin-right: 0;
}
map {
  margin-left: 32rpx;
  height: 218rpx;
  width: 686rpx;
  border: 1rpx solid #e8e8e8;
}
.view3 {
  padding: 20rpx 32rpx;
  font-size: 27rpx;
  line-height: 40.5rpx;
}
.v3-row {
  height: 80rpx;
}
.v3-row view:first-child {
  margin-right: 16rpx;
  color: #0ccef1;
}

.view4 {
  color: #4a4a4a;
  font-size: 28rpx;
  line-height: 42rpx;
  padding: 32rpx;
}
.view4 image{
  width: 38rpx;
  height: 24rpx;
}
.v4-title {
  font-size: 40rpx;
  line-height: 60rpx;
  margin-bottom: 20rpx;
}
.v4-des{
   text-indent:56rpx;
   display: -webkit-box !important; 
   overflow: hidden;
   text-overflow: ellipsis;
   word-break: break-all;
  -webkit-box-orient: vertical; 
  -webkit-line-clamp: 3;
  margin-bottom: 20rpx;
}
.v4-des_change{
  text-indent:56rpx;
  margin-bottom: 20rpx;
}
.v4-jt{
    width: 80rpx;
    height: 80rpx;
}
.view5{
  padding: 32rpx;
}
.v5-title{
  font-size: 40rpx;
  line-height: 60rpx;
}
.v5-row{
  margin-top: 32rpx;
}
.v5-icon{
  width: 81rpx;
  height: 81rpx;
  background-color: #d8d8d8;
  border-radius: 40.5rpx;
  margin-right: 32rpx;
}
.v5-icon image{
  width: 81rpx;
  height: 81rpx;
  border-radius: 40.5rpx;
}
.v5-d1s{
  margin-left: 0rpx;
  margin-bottom: 14rpx;
}
.v5-d1 view:nth-child(1){
  font-size: 28rpx;
  line-height: 42rpx;
}
.v5-d1 view:nth-child(2){
  font-size: 24rpx;
  line-height: 36rpx;
  color: #a6a6a6;
}
.v5-text{
  font-size: 28rpx;
  line-height: 34rpx;
  color: #656565;
  margin-top: 6rpx;
  margin-bottom: 32rpx;
}

.view6{
  margin-top: 64rpx;
  margin-bottom: 128rpx;
}
.btnall{
  width: 248rpx;
  height: 64rpx;
  font-size: 28rpx;
  line-height: 42rpx;
  border: 3rpx solid #0ccef1;
  color: #0ccef1;
}

.docomm{
  background-color: #40c8de;
  height: 88rpx;
  color: white;
  font-size: 32rpx;
  line-height: 48rpx;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
}
.goodsline {
  width: 646rpx;
  height: 2rpx;
  margin-left: 40rpx;
  background-color: rgb(235, 235, 235);
}